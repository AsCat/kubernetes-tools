__tmux() {
    # get session name
    tmux_session_name=$1

    # start a new session
    tmux -u new-session -d -s $tmux_session_name

    # send first command
    tmux send-keys "${commands[1]}" Enter
    # remove the first element in the array

    # loop through the rest of the commands
    for command in "${commands[@]:2}"
    do
        # start a new pane
        tmux split-window -t :0 -h
        # send command to pane
        tmux send-keys "$command" Enter
        # change layout to tiles
        tmux select-layout -t :0 tiled > /dev/null
    done

    # select the first pane
    tmux select-pane -t 0

    # sync input
    tmux set-window-option -t :0  synchronize-panes on > /dev/null

    # attach tmux session
    tmux attach -dt $tmux_session_name
}